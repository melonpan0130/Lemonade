html
    head
        script. 
    body
        
        h1 Lemonade -#{userName}'s Page
        a(href="/insert/")
            button New
        ul
            each item in data
                li
                    a(href='/lemon/'+item[0]+'/'+item[1]): h3= item[2]
                    p= item[3]
                    //- img= item[4]
                    a(href='/updateBoard/'+item[1])
                        button Edit
                    a(href='/deleteBoard/'+item[1])
                        button Delete
        
        h3 Wish list
        ul
            each item in wish
                li
                    a(href='/lemon/'+item[0]+'/'+item[1]): h3= item[2]
                    p= item[3]
                    //- img= item[4]
                    a(href='/deleteHeart/'+item[0]+'/'+item[1])
                        button Delete